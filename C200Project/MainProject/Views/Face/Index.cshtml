<!DOCTYPE html>
<html>
<head>
   <title>
      Webcam Login Demo
   </title>
   <meta name="viewport" content="width=device-width" />
   <title>Where We Were</title>
   <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
   <script src="~/lib/jquery/jquery.min.js"></script>
   <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
   <script src="~/lib/upload.js"></script>
   <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
   <style>
      #vid-show, #vid-canvas, #vid-take {
         margin-bottom: 20px;
      }

      html, body {
         padding: 0;
         margin: 0;
      }
   </style>
</head>
<body>
    <div class="container">
            <h1>Face Login</h1>
            <div id="vid-controls" class="col-sm-4">
                <video id="vid-show" autoplay width="320" height="240"></video>
                <input id="vid-take" type="button" value="Login" class="btn btn-primary" />
                <div id="vid-canvas"></div>
            </div>
    </div>
    @if (TempData["Message"] != null)
    {
        <div class="alert alert-info">
            @TempData["Message"]
        </div>
    }

</body>
</html>